<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>AllowedTypes</key>
	<array>
		<string>public.swift-source</string>
	</array>
	<key>DefaultCompletionName</key>
	<string>MVVM Router Swift</string>
	<key>Description</key>
	<string>Generates a View Controller, View Xib, ViewModel and Router</string>
	<key>Kind</key>
	<string>Xcode.IDEFoundation.TextSubstitutionFileTemplateKind</string>
	<key>MainTemplateFiles</key>
	<array>
		<string>___FILEBASENAME___ViewController.xib</string>
		<string>___FILEBASENAME___ViewController.swift</string>
		<string>___FILEBASENAME___ViewModel.swift</string>
		<string>___FILEBASENAME___Router.swift</string>
	</array>
	<key>Options</key>
	<array>
		<dict>
			<key>Description</key>
			<string>The name of the module to create</string>
			<key>Identifier</key>
			<string>productName</string>
			<key>Name</key>
			<string>Class name</string>
			<key>NotPersisted</key>
			<true/>
			<key>Required</key>
			<true/>
			<key>Type</key>
			<string>text</string>
		</dict>
		<dict>
			<key>Default</key>
			<string>Swift</string>
			<key>Description</key>
			<string>The language of template</string>
			<key>Identifier</key>
			<string>templateLanguage</string>
			<key>Name</key>
			<string>Language:</string>
			<key>Required</key>
			<false/>
			<key>Type</key>
			<string>static</string>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>viewControllerName</string>
			<key>Name</key>
			<string>ViewController:</string>
			<key>Default</key>
			<string>$(PRODUCT_NAME)</string>
			<key>Type</key>
			<string>_</string>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>viewModelName</string>
			<key>Name</key>
			<string>ViewModel:</string>
			<key>Default</key>
			<string>$(PRODUCT_NAME)</string>
			<key>Type</key>
			<string>_</string>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>routerName</string>
			<key>Name</key>
			<string>Router:</string>
			<key>Default</key>
			<string>$(PRODUCT_NAME)</string>
			<key>Type</key>
			<string>_</string>
		</dict>
	</array>
	<key>Summary</key>
	<string>Templates for Swift using MVVM pattern with Routers</string>
</dict>
</plist>
